#include<stdio.h>
main()

{
	int a[3][3],i,j;
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("Enter number in pocket [%d][%d]",i,j);
			scanf("%d",&a[i][j]);
		}
	}
	printf("MATRIX IS..........\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%5d",a[i][j]);
		}
		printf("\n");
	}
	
}
